<h3 class="middle search-margin">Search Result</h3>  

    <% @results.each do |result| %>  
    <div class="container">
  <div class="myaccordion">
    <div class="card">
      <div class="card-header middle">
        <h2 class="mb-0">
          <button class=" btn btn-link">
            <%= link_to "#{result.description}", post_path(result) %><br>  
          </button>
        </h2>
      </div>
    </div>
  </div>
</div>
    <% end %>