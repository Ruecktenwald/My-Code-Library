<% if user_signed_in? %>
  <%= form_tag(search_page_path, :method => "get") do %>  
    <div class="has-search middle" id="search-input">  
      <%= search_field_tag :search, params[:search], placeholder: "Search", autofocus: true, class: "search"%> 
      <%= button_tag "Find my code", :class => ' btn-red btn-size',:name => nil %>  
    </div>  
  <% end %>
<% end %>